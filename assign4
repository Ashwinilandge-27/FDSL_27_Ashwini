#include<stdio.h>
int main()
{
int n,i,j,k,temp;
printf("Enter the size of array");
scanf("%d",&n);
int a[n];
 printf("Enter the array elements");
 for(i=0;i<n;i++){
 scanf("%d",&a[i]);
 }
 for(i=1;i<n-1;i++)
 {
 temp=a[i];
 k=i+1;
 for(j=k;j>=0;j--)
 {
 if (a[j]>temp)
 {
 a[j+1]=a[j];
 }
 
 else{
 break;
 }
 a[j+1]=temp;
 }
 }
 printf("\n Sorted Array is:");
 for(i=0;i<n;i++)
 {
 printf("%d\t",a[i]);
 }
 return 0;
}
 
