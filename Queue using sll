#include<stdio.h>
#include<stdlib.h>
struct node
{
 int data;
 struct node *next;
 };
 struct node *rear= NULL,*front= NULL;
 struct node *temp,*newnode,*rear,*front,*prevnode;
 void enQueue() {
    struct node *newNode = malloc(sizeof(struct node));
    struct node *temp=rear;
    printf("Enter data for new node: \n");
    scanf("%d", &newNode->data);
    
    newNode->next = NULL;
    
    if (rear == NULL) {
        rear = newnode;
    } else {
        temp = rear;
        while (temp->next != NULL) {
            temp = temp->next;
        }
        temp->next = newNode;
    }
}
 void deQueue() {
    if (front == NULL) {
        printf("The list is empty.\n");
    } else {
        temp = front;
        front = front->next;
        temp->next = NULL;
        free(temp);
    }
}
 void display() {
    temp=front,rear;
    while (temp != NULL) {
        printf("%d", temp->data);
        temp = temp->next;
    }
   }
  
  int main(){
     int ch;
     rear,front=NULL;
     struct node *newNode = malloc(sizeof(struct node));
     while (ch!=0) {
        printf("\nEnter your choice:\n 1. Enqueue\n 2. Dequque\n 3.Display\n 4.Exit\n");
        scanf("%d", &ch);
     
     switch(ch){
     case 1: 
            enQueue();
            break;
     case 2: 
            deQueue();
            break;
     case 3:
            display();
            break;
     case 4:
            return 0;
            break;
     default:
            printf("\nInvalid choice!\n");
     }
    }
return 0;
}    
           
     
 
 
